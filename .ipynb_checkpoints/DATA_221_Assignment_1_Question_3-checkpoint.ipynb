   "source": [
    "def find_power_of_num(pairs):\n",
    "    power_list = []\n",
    "    for base, exponent in pairs:\n",
    "        if exponent >= 0:\n",
    "            power_list.append(base ** exponent)\n",
    "    return power_list\n",
    "\n",
    "pairs = [[5, 2], [3, -1], [4, 3], [2, 0]]\n",
    "\n",
    "print(find_power_of_num(pairs))"
   ]
