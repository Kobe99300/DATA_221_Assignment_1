   "source": [
    "threshold = 100\n",
    "product = 1\n",
    "currentNumber = 1\n",
    "\n",
    "while product <= threshold:\n",
    "    product *= currentNumber\n",
    "    currentNumber += 1\n",
    "\n",
    "print(f\"the final product is {product} and the integer that caused the product to exceed the threshold was {currentNumber}.\") "
